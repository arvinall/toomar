{
  "name": "Toomar",
  "includeVersion": true,
  "entryPointStrategy": "expand",
  "entryPoints": ["lib/*.d.ts", "lib/*/*.d.ts"],
  "readme": "README.md",
  "out": "docs",
  
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "cleanOutputDir": true,
  "searchInComments": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": false
  },
  "categorizeByGroup": false,
  "githubPages": false,

  "kindSortOrder": ["Function"],

  "categoryOrder": [
    "listen",
    "Configs",
    "Units",
    "Operators",
    "Locators",
    "Transformers",
    "Schedulers",
    "Filters",
    "*",
    "Internals"
  ],

  "plugin": ["typedoc-plugin-merge-modules"],

  "navigationLinks": { "Github": "https://github.com/arvinall/toomar" },
  "sidebarLinks": {
    "Website": "https://toomar.js.org",
    "npm": "https://www.npmjs.com/package/toomar"
  }
}
