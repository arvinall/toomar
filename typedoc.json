{
  "name": "Toomar",
  "includeVersion": true,
  "entryPointStrategy": "expand",
  "entryPoints": ["lib/*.d.ts", "lib/*/*.d.ts"],
  "readme": "README.md",
  "out": "docs",
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "cleanOutputDir": true,
  "searchInComments": true,
  "kindSortOrder": ["Function"],
  "navigation": {
    "includeCategories": true,
    "includeGroups": false
  },
  "categorizeByGroup": false,
  "categoryOrder": [
    "listen",
    "Configs",
    "Units",
    "Operators",
    "Locators",
    "Transformers",
    "Schedulers",
    "Filters",
    "*",
    "Internals"
  ],
  "plugin": ["typedoc-plugin-merge-modules"]
}
